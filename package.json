{
  "name": "observame",
  "description": "Simple reactive library",
  "version": "1.0.0",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/observame.umd.cjs",
  "module": "./dist/observame.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/observame.js",
      "require": "./dist/observame.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evuz/observame.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc",
    "lint": "eslint src/**",
    "lint:format": "eslint --fix src/**",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "type:check": "tsc",
    "type:check:watch": "tsc --watch"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.54.0",
    "@typescript-eslint/parser": "5.54.0",
    "eslint": "8.35.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-n": "15.6.1",
    "eslint-plugin-promise": "6.1.1",
    "typescript": "4.9.3",
    "vite": "4.1.0",
    "vitest": "^0.29.2"
  }
}
